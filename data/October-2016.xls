				if ((ed.value !== WATER_END) && (ed.value.length > 0) && (ed.value > sd.value)) {
					redirectUrl += encodeURIComponent(ed.value);
				} else {
					redirectUrl += encodeURIComponent(sd.value);
				}
				window.open(redirectUrl);
			}
		}
	});

